<div class="container text-center">
<h1 class='my-5'>Index of all the lists</h1>
<div class='container d-flex flex-column'>
<% @lists.each do |list| %>
  <%= link_to list.name, list_path(list)%>
  <%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill %>
<% end %>
</div>
<p ><%= link_to "Create a new list", new_list_path %></p>
</div>
